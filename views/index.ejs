            <% if(events) events.forEach(event=>{%>
              <div class="events-list-item live">
                <div class="event-date"><strong><%= event.date%> - </strong> <%= event.title.subHeading%></div>
                <div class="event-name"><%= event.title.heading%></div>
                <div class="event-tags">
                  <ul>
                    <% event.tags.forEach((tag)=>{%>
                      <li><%=tag%></li>
                    <%})%>
                  </ul>
                </div>
                <p><%= event.description%></p>
                <img src="<%=event.img%>">
                
                <button onclick="window.open('<%=event.link%>', '_blank')">View Details</button>
              </div>
              <%})%>
            <%if(!events){%>
              <h4 style="text-align: center;">No upcoming events.</h4>
              <%}%>
          </div>
        </div>
      
