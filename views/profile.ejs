  <body>
    <%-include('partials/navbar')%>
    <!-- Edit Expereince Modal  -->

    <!-- Flash Message Body Start -->
    <%-include('partials/flashMessage')%>
    <!-- Flash Message body end -->
    <div class="addExperienceModal" id="addExperienceModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo1"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt="background logo"
            style="cursor: pointer;"
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span> Experience</h2>

          <form
            class="subscribeForm"
            method="post"
            action="/user/experience"
            onsubmit="return addExperienceModalvalidate()"
            name="addExperienceform"
          >
            <div class="flexRow">
              <label for="exp_name"> Company Name</label>
              <input id="exp_name" type="text" name="exp_name" />
            </div>
            <div class="flexRow">
              <label for="exp_role"> Job Role</label>
              <input id="exp_role" type="text" name="exp_role" />
            </div>
            <div class="flexRow">
              <label for="exp_status">Status</label>
              <select
                name="exp_status"
                id="exp_status"
                onchange="statusListener()"
              >
                <option value="Completed" selected>Completed</option>
                <option value="Ongoing">Ongoing</option>
              </select>
            </div>
            <div class="flexRow">
              <label for="exp_startdate">Start Date</label>
              <input
                id="exp_startdate"
                type="date"
                placeholder="dd/mm/yyyy"
                name="exp_startdate"
                required
              />
            </div>
            <div class="flexRow">
              <label for="exp_enddate">End Date</label>
              <input
                id="exp_enddate"
                placeholder="dd/mm/yyyy"
                type="date"
                name="exp_enddate"
              />
            </div>

            <div class="flexRow">
              <label for="exp_description">Description</label>
              <input
                id="Description"
                type="text"
                name="exp_description"
                maxlength="100"
              />
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input
                id="submit_experience"
                class="submit"
                type="submit"
                value="Add Experience"
              />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- Edit Experience Modal  -->
    <!-- Edit Profile Modal  -->
    <div class="editProfileModal" id="editProfileModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo"
            class="backlogo"
            src="/img/logo/bac.png"
            alt="background logo"
            style="cursor: pointer;"
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox" style="justify-content: space-around">
          <h2><span class="thin"></span></h2>
          <div class="socials">
            <div class="items">
              <div class="photo">
                <% if(user.profileImageLocation!=null){ %>
                <img
                  src="/upload<%= user.profileImageLocation%>"
                  alt="profile picture"
                />
                <% }else{%> <img src="/img/logo/profile.png" alt="avatar" />
                <% }%>
                </div>
              <form
                style="width: 100%; padding: 0px auto"
                action="profile/upload/<%=user._id%>"
                method="post"
                enctype="multipart/form-data"
                class="uploadImageForm"
              >
                <div class="upload-btn-wrapper">
                  <label style="width: 200px" class="custom-file-upload">
                    <input
                      type="file"
                      name="profile-image"
                      value=""
                      id="fileSubmitButton"
                      onchange="didyouupload()"
                    />
                    <span> Choose Photo</span>
                  </label>
                  <label id="file-name"></label>
                  <input
                    id="uploadphoto"
                    style="
                      width: 200px;
                      height: 40px;
                      color: rgb(238, 238, 238) !important;
                      opacity: 0;
                    "
                    type="submit"
                    class="submit"
                    value="Upload"
                  />
                </div>
              </form>
            </div>
            <div class="items link">
              <nobr style="display: flex; align-items: center">
                <img src="/img/logo/link.png" alt="Linkedin logo" />
                <a href=""
                  >Connect to Linkedin <br />
                  <span>(Feature Coming Soon)</span></a
                >
              </nobr>
            </div>
            <div class="items twitter">
              <nobr style="display: flex; align-items: center">
                <img src="/img/logo/twitter.png" alt="twitter logo" />
                <a href="#"
                  >Connect to Twitter<br />
                  <span>(Feature Coming Soon)</span></a
                >
              </nobr>
            </div>
          </div>
          <p></p>
          <img
            class="edill"
            src="/img/illustrations/editing.png"
            alt="illustration"
          />
        </div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <form
            class="subscribeForm"
            method="post"
            action="/user/profile"
            onsubmit="return editProfileModalvalidate()"
            name="editprofileform"
          >
            <div class="flexRow">
              <label for="name">Name</label>
              <input
                id="name"
                type="text"
                value="<%=user.name%>"
                name="name"
                required
                maxlength="30"
              />
            </div>
            <hr size="1px" />
            <div class="flexRow">
              <label for="degree">Degree</label>
              <select name="degree" id="degree">
                <%
                    var options = [ "", "Bachelor of Technology", "Master of Technology", "PHD"];
                    options.forEach(option=>{
                        let selected = (user.degree === option) ? "selected" : "";
                      %><option value="<%=option %>" <%=selected %>>
                  <% if(option === "") { %>
                  Select
                  <% } else { %>
                  <%= option %>
                  <% } %>
                </option><%
                    })
                  %>
              </select>
            </div>
            <div class="flexRow">
              <label for="branch">Branch</label>
              <select name="branch" id="branch">
                <%
                    var options = [
                     "", 
                     "Computer Science and Engineering", 
                     "Electronics and Communication Engineering", 
                     "Electrical Engineering",
                     "Electronics & Instrumentation",
                     "Mechanical Engineering",
                     "Civil Engineering"
                    ];
                    options.forEach(option=>{
                        let selected = (user.branch === option) ? "selected" : "";
                      %><option value="<%=option %>" <%=selected %>>
                  <% if(option === "") { %>
                  Select
                  <% } else { %>
                  <%= option %>
                  <% } %>
                </option><%
                    })
                  %>
              </select>
            </div>
            <div class="flexRow">
              <label for="batchYear">Batch Year</label>
              <select name="batchYear" id="batchYear">
                <%
                  var options = [
                   "", 
                   "2020", 
                   "2021", 
                   "2022",
                   "2023",
                   "2024"
                  ];
                  options.forEach(option=>{
                      let selected = (user.batch === option) ? "selected" : "";
                    %><option value="<%=option %>" <%=selected %>>
                  <% if(option === "") { %>
                  Select
                  <% } else { %>
                  <%= option %>
                  <% } %>
                </option><%
                  })
                %>
              </select>
            </div>
            <h5 class="footnote">Please select the expected year of passing</h5>
            <hr size="1px" />
            <div class="flexRow">
              <label for="bio">Bio</label>
              <input
                id="bio"
                type="text"
                placeholder="Something about you."
                name="bio"
                value="<% if(user.bio){%><%=user.bio%><%}%>"
                maxlength="50"
              />
            </div>
            <div class="flexRow">
              <label for="facebook"> Facebook </label>
              <input
                id="facebook"
                type="url"
                value="<% if(user.socialLinks['facebook']){%><%=user.socialLinks['facebook']%><%}%>"
                placeholder="Enter Your Facebook account URL"
                name="facebook"
                maxlength="200"
              />
            </div>
            <div class="flexRow">
              <label for="linkedin"> LinkedIn </label>
              <input
                id="linkedin"
                type="url"
                value="<% if(user.socialLinks['linkedin']){%><%=user.socialLinks['linkedin']%><%}%>"
                placeholder="Enter Your Linkedin account URL"
                name="linkedin"
                maxlength="200"
              />
            </div>

            <div class="flexRow">
              <label for="github"> Github </label>
              <input
                id="github"
                type="url"
                value="<% if(user.socialLinks['github']){%><%=user.socialLinks['github']%><%}%>"
                placeholder="Enter Your github profile URL"
                name="github"
                maxlength="200"
              />
            </div>
            <hr size="1px" />
            <div class="flexRow">
              <label for="email">Email</label>
              <input
                id="email"
                type="email"
                value="<%= user.email %>"
                name="email"
                disabled
              />
            </div>
            <div class="flexRow">
              <label for="password">Password </label>
              <div class="flexRow fullwidth">
                <!-- <p>Last Changed 2 months ago</p> -->
                <a href="/user/update-password">Change Password</a>
              </div>
            </div>
            <br /><br />
            <div class="flexRow fullwidth" style="justify-content: center">
              <input
                id="submit_editprofile"
                class="submit"
                type="submit"
                value="Save Changes"
                required
              />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- Edit Profile Modal  -->

    <!-- Image Viewer Modal  -->
    <div class="modal">
      <img class="fullview" src="" alt="modal" />
    </div>
    <!-- Image Viewer Modal  -->

    <!-- addSkillsModal Modal  -->
    <div class="addSkillsModal" id="addSkillsModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo2"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt="background logo"
            style="cursor: pointer;"
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span> Skills</h2>

          <form
            class="subscribeForm"
            method="post"
            action="/user/skill"
            name="addSkillsForm"
            onsubmit="return addSkillsModalvalidate()"
          >
            <div class="flexRow">
              <label for="skillname">Skill Name</label>
              <input
                id="skillname"
                type="text"
                name="skillname"
                maxlength="30"
                required
              />
            </div>
            <div class="flexRow">
              <label for="skilllevel">Skill Level</label>
              <select name="skilllevel" id="skilllevel" required>
                <option value="Beginner" selected>Beginner</option>
                <option value="Intermediate">Intermediate</option>
                <option value="Expert">Expert</option>
              </select>
            </div>
            <!-- <div class="flexRow">
              <label for="skillduration">Duration</label>
              <input
                id="skillsduration"
                placeholder="In months"
                name="skillduration"
                maxlength="5"
              />
            </div> -->
            <div class="flexRow">
              <label for="skilldescription">Description</label>
              <input
                id="skilldescription"
                type="text"
                name="skilldescription"
                maxlength="100"
              />
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input
                id="submit_skills"
                class="submit"
                type="submit"
                value="Save Changes"
              />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- addSkillsModal Modal  -->

    <!-- addProjectsModal Modal  -->
    <div class="addProjectsModal" id="addProjectsModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo3"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt="background logo"
            style="cursor: pointer;"
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span>Personal Project</h2>

          <form
            class="subscribeForm"
            method="post"
            action="/user/project/personal"
            name="addProjectForm"
            onsubmit="return addProjectModalvalidate()"
          >
            <div class="flexRow">
              <label for="projectname">Project Name</label>
              <input
                id="projectname"
                type="text"
                name="title"
                required
                maxlength="50"
              />
            </div>
            <div class="flexRow">
              <label for="projectrole">Project Role</label>
              <input
                id="projectrole"
                type="text"
                name="role"
                maxlength="30"
                required
              />
            </div>
            <div class="flexRow">
              <label for="status">Project Status</label>
              <select
                name="status"
                id="projectStatus"
                required
                onchange="statusListenerProject()"
              >
                <option value="Completed" selected>Completed</option>
                <option value="Ongoing">Ongoing</option>
              </select>
            </div>
            <div class="flexRow">
              <label for="startdate">Start Date</label>
              <input id="startdate" type="date" name="startdate" required />
            </div>
            <div class="flexRow">
              <label for="enddate">End Date</label>
              <input id="enddate" type="date" name="enddate" required />
            </div>

            <div class="flexRow">
              <label for="techstack">Techstacks</label>
              <input
                id="techstack"
                placeholder="Tech Stack"
                type="text"
                name="techstack"
                maxlength="30"
                required
              />
            </div>
            <div class="flexRow">
              <label for="githuburl">Git Hub URL</label>
              <input
                id="githuburl"
                type="url"
                placeholder="Your Github repository link"
                name="githuburl"
              />
            </div>
            <div class="flexRow">
              <label for="hosturl">Hosted URL</label>
              <input
                id="hosturl"
                type="url"
                placeholder="Your Hosted  link"
                name="hosturl"
              />
            </div>
            <div class="flexRow">
              <label for="description">Description</label>
              <input
                id="description"
                type="text"
                name="description"
                maxlength="100"
                required
              />
            </div>
            <div class="flexRow">
              <label for="description" class="fullwidth" >Do you need Collaboration in this project? </label>
              <input
              id="askCollaboration"
              type="checkbox"
              name="askCollaboration"        
              checked
            /> 
              
                            
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input
                id="submit_project"
                class="submit"
                type="submit"
                value="Add Project"
              />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- addSkillsModal Modal  -->
    <!-- addAchievementModal Modal  -->
    <div class="addAchievementModal" id="addAchievementModal">
      <div class="wrapper backPart">
        <div id="subscribeBox">
          <img
            id="backlogo4"
            class="backlogo1"
            src="/img/logo/bac.png"
            alt="background logo"
            style="cursor: pointer;"
          />
        </div>
        <div></div>
      </div>
      <div class="wrapper">
        <div id="subscribeBox">
          <h2><span class="thin">Add a new </span> Achievement</h2>

          <form
            class="subscribeForm"
            method="POST"
            action="/user/achievement"
            onsubmit="return addAchievementModalvalidate()"
            name="addAchievementForm"
          >
            <div class="flexRow">
              <label for="achievementname">Achievement Name</label>
              <input
                id="achievementname"
                type="text"
                name="achievementname"
                maxlength="50"
                required
              />
            </div>
            <div class="flexRow">
              <label for="achievementnameplatform">Achievement Platform</label>
              <input
                id="achievementnameplatform"
                type="text"
                name="achievementnameplatform"
                maxlength="40"
                required
              />
            </div>
            <div class="flexRow">
              <label for="achievementnamedate">Received Date</label>
              <input
                id="achievementnamedate"
                type="date"
                name="achievementnamedate"
                required
              />
            </div>
            <div class="flexRow">
              <label for="achievementnameserial">Unique Serial Number</label>
              <input
                id="achievementnameserial"
                type="text"
                placeholder="Unique Certification Number"
                name="achievementnameserial"
                maxlength="50"
              />
            </div>
            <br /><br />
            <div class="flexRow fullwidth">
              <input
                id="submit_achievement"
                class="submit"
                type="submit"
                value="Add Achievement"
              />
            </div>
          </form>
        </div>
      </div>
    </div>
    <!-- addAchievementModal Modal  -->
